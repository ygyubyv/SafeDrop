{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "routes": [
    {
      "route": "/create",
      "rewrite": "/index.html"
    },
    {
      "route": "/load/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/link/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
